<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.ml_heroescounters">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/actionbaractivityrole"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".activity_heroe_selected"
                  android:label="@string/actionbaractivityheroeselected"
                  android:parentActivityName=".activity_role_tank"
                  android:theme="@style/AppTheme">
        </activity>
        <activity
                android:name=".activity_role_tank"
                android:label="@string/actionbaractivitytank"
                android:parentActivityName=".MainActivity"
                android:theme="@style/AppTheme">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>